<Application
    x:Class="CommandBarIssues.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:CommandBarIssues">

    <Application.Resources>
        <ResourceDictionary>
            <SolidColorBrush x:Key="CommandBarForeground" Color="Red"/>
            <SolidColorBrush x:Key="AppBarEllipsisButtonForeground" Color="Green"/>
            <SolidColorBrush x:Key="AppBarEllipsisButtonForegroundPointerOver" Color="Orange"/>
            <SolidColorBrush x:Key="AppBarEllipsisButtonForegroundPressed" Color="Blue"/>
            <SolidColorBrush x:Key="AppBarButtonForeground" Color="Green"/>
            <SolidColorBrush x:Key="AppBarButtonForegroundPointerOver" Color="Cyan"/>
            <SolidColorBrush x:Key="AppBarButtonForegroundPressed" Color="White"/>
        </ResourceDictionary>
    </Application.Resources>
</Application>
